<?xml version="1.0" encoding="UTF-8" ?>
<ContentPage
    xmlns="http://xamarin.com/schemas/2014/forms"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    x:Class="NextPlayGround.Landing">
    <ContentPage.Content>
        <StackLayout>
            <Button Text="Go to carousel" Clicked="Button_Clicked" />
            <Button Text="Go to Animation" Clicked="Button2_Clicked" />
            <Button Text="Go to VSM" Clicked="VSM_Clicked" />
            <Button Text="Go to picker" Clicked="Picker_Clicked" />
            <Button Text="Go to webview home" Clicked="WebView1_Clicked" />
            <Button Text="Go to webview inner content" Clicked="WebView2_Clicked" />
            <WebView x:Name="Web" VerticalOptions="FillAndExpand" />                     

        </StackLayout>
    </ContentPage.Content>
</ContentPage>
